<p id="notice"><%= notice %></p>

<h1>Foldback View</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Time</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @movements.each do |movement| %>
      <tr>
        <td><%= movement.name %></td>
        <td><%= movement.time %></td>
      </tr>
    <% end %>
  </tbody>
</table>
